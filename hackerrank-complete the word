#include<stdio.h>
#include<string.h>
int main()
{
	char str1[100],str2[100];
	int i,j=0,c=0;
	gets(str1);
	gets(str2);
	for(i=0;str1[i]!='\0';i++)
	{
		c=0;
		for(j=j;str2[j]!='\0';j++)
		{
			if(str1[i]==str2[j])
			{
				c++;
				break;
			}
		}
	}
	if(c==0)
	printf("0");
	else
	printf("1");
	return 0;
}
